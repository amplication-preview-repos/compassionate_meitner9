datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Comment {
  comment     String?
  createdAt   DateTime @default(now())
  id          String   @id @default(cuid())
  orderId     Int?
  reportCount Int?
  updatedAt   DateTime @updatedAt
  userId      Int?
}
